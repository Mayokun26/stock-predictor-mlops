{
  "dashboard": {
    "id": null,
    "title": "MLOps System Overview",
    "description": "Comprehensive monitoring for MLOps stock prediction system",
    "tags": ["mlops", "stock-prediction", "production"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "API Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"mlops-api\"}",
            "legendFormat": "API Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Predictions per Hour",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(predictions_total[1h]) * 3600",
            "legendFormat": "Predictions/hour"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Model Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "model_accuracy",
            "legendFormat": "{{model_version}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Data Drift Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "data_drift_score",
            "legendFormat": "{{feature}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.25}
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 0.5
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "API Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(prediction_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prediction_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prediction_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Prediction Volume by Model",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(predictions_total[5m]) * 300",
            "legendFormat": "{{model_version}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 7,
        "title": "Feature Store Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(feature_store_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(feature_store_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(prediction_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container!=\"\"}[5m]) * 100",
            "legendFormat": "CPU Usage % - {{container}}"
          },
          {
            "expr": "container_memory_usage_bytes{container!=\"\"} / container_spec_memory_limit_bytes * 100",
            "legendFormat": "Memory Usage % - {{container}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent"
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 10,
        "title": "Database Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "postgresql_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 26}
      },
      {
        "id": 11,
        "title": "Redis Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 26}
      },
      {
        "id": 12,
        "title": "A/B Test Results",
        "type": "table",
        "targets": [
          {
            "expr": "ab_test_sample_size",
            "legendFormat": "{{variant}}",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "table"
            }
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 26}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "model_version",
          "type": "query",
          "query": "label_values(predictions_total, model_version)",
          "refresh": "on_time_range_change"
        },
        {
          "name": "symbol",
          "type": "query", 
          "query": "label_values(predictions_total, symbol)",
          "refresh": "on_time_range_change"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Model Deployments",
          "datasource": "prometheus",
          "expr": "increase(model_deployment_events_total[1h])"
        }
      ]
    }
  }
}