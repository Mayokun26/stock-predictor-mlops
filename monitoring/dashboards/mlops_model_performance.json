{
  "dashboard": {
    "id": null,
    "title": "MLOps Model Performance Dashboard",
    "tags": ["mlops", "models", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "model_version",
          "type": "query",
          "query": "label_values(predictions_total, model_version)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "symbol",
          "type": "query", 
          "query": "label_values(predictions_total, symbol)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Prediction Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(predictions_total[$__rate_interval])",
            "legendFormat": "Predictions/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Model Accuracy by Version",
        "type": "timeseries",
        "targets": [
          {
            "expr": "model_accuracy{model_version=~\"$model_version\"}",
            "legendFormat": "{{model_version}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Current Model Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(model_accuracy)",
            "legendFormat": "Accuracy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.65}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Prediction Latency Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, prediction_request_duration_seconds_bucket)",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, prediction_request_duration_seconds_bucket)",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Feature Store Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(feature_store_cache_hits_total[$__rate_interval])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(feature_store_cache_misses_total[$__rate_interval])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Data Drift Detection",
        "type": "timeseries",
        "targets": [
          {
            "expr": "data_drift_score",
            "legendFormat": "{{feature}} drift"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.25}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Prediction Errors by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (error_type) (prediction_errors_total)",
            "legendFormat": "{{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Business Metrics - Profit Factor",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(profit_factor_metric)",
            "legendFormat": "Profit Factor"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1.0},
                {"color": "green", "value": 1.5}
              ]
            },
            "unit": "short",
            "decimals": 2
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Sharpe Ratio Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sharpe_ratio_metric{model_version=~\"$model_version\"}",
            "legendFormat": "{{model_version}} Sharpe"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 24}
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Model Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(model_deployment_timestamp[1h]) > 0",
          "iconColor": "green",
          "titleFormat": "Model Deployment: {{model_version}}"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}